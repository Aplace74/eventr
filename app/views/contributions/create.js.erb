function refreshUpdateForm(innerHTML) {
  const supplyCard = document.querySelector("[data-supply-id='<%= @supply.id %>']");
  supplyCard.innerHTML = innerHTML;
  refreshJs(supplyCard);
}
refreshUpdateForm('<%= j render "supply_card", supply: @supply, participation: @participation, contribution: @participation.contributions.find_by(supply_id: @supply.id)%>')